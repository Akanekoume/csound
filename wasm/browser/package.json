{
  "name": "@csound/browser",
  "version": "0.0.1",
  "main": "./dist/libcsound.mjs",
  "type": "module",
  "typings": "index.d.ts",
  "keywords": [
    "csound",
    "csound-wasm",
    "wasm",
    "webassembly",
    "audio",
    "webaudio",
    "wasi"
  ],
  "scripts": {
    "lint": "eslint 'src/**/*.js'",
    "build": "yarn build:dev && yarn build:prod",
    "build:dev": "cross-env BUILD_TARGET=development rollup -c --environment production",
    "build:prod": "cross-env BUILD_TARGET=production rollup -c --environment production",
    "build:readme": "node script/generate-readme.js > README.md",
    "test:ci": "node tests/selenium_runner.js",
    "test:open": "http-server ./tests -c-1 --no-dotfiles",
    "build:types": "jsdoc -t node_modules/tsd-jsdoc/dist -r src -d .; mv types.d.ts index.d.ts"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-transform-destructuring": "^7.12.1",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-replace": "^2.3.4",
    "babel-plugin-add-module-exports": "^1.0.4",
    "browser-or-node": "^1.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-ramda": "^2.5.1",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-unicorn": "^26.0.1",
    "http-server": "^0.12.3",
    "jsdoc": "^3.6.6",
    "jsdoc-to-markdown": "^6.0.1",
    "ololog": "^1.1.164",
    "rollup": "^2.36.1",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-pluginutils": "^2.8.2",
    "tsd-jsdoc": "^2.5.0"
  },
  "dependencies": {
    "@csound/wasm": "../",
    "@rollup/plugin-strip": "^2.0.0",
    "@wasmer/wasi": "^0.12.0",
    "@wasmer/wasm-transformer": "^0.12.0",
    "@wasmer/wasmfs": "0.12.0",
    "comlink": "^4.3.0",
    "eventemitter3": "^4.0.7",
    "log": "^6.0.0",
    "mocha": "^8.2.1",
    "mocha-webdriver-runner": "^0.6.3",
    "pako": "^2.0.3",
    "path-browserify": "^1.0.1",
    "ramda": "^0.27.1",
    "selenium-webdriver": "^4.0.0-alpha.8",
    "standardized-audio-context": "^25.1.8",
    "text-encoding-shim": "^1.0.5",
    "unmute-ios-audio": "^3.3.0",
    "web-midi-api": "^2.0.8"
  }
}
